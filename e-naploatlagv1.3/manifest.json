{
	"manifest_version": 2,
	
	"name": "E-Napló Átlag",
	"description": "Ez a bővítmény a Szent Angéla Ferences Ált. Isk. és Gimn. E-Naplójához készült, azzal a céllal, hogy megkönnyítse a diákok életét.",
	"version": "1.3",
	"options_page": "about.html",
	
	"browser_action": {
		"default_icon": "images/icon16.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [
	{
		"matches": ["https://www.szentangela.sulinet.hu/*", "http://www.szentangela.sulinet.hu/*", "*://www.szentangela.sulinet.hu/*"],
		"js": ["jquery-3.2.1.js", "rewrite.js"]
	}
	],
	"icons": { 
		"16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png" 
	}
}